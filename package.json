{
  "name": "@sweetgreen/sg-node-dynamodb",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/@sweetgreen"
  },
  "version": "0.1.3",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "description": "SG node dynamodb",
  "repository": "git://github.com/sweetgreen/sg-node-dynamodb.git",
  "license": "Internal",
  "author": "Sweetgreen",
  "devDependencies": {
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/github": "^7.2.3",
    "@semantic-release/npm": "^7.1.3",
    "@semantic-release/release-notes-generator": "^9.0.3",
    "@types/dotenv": "^8.2.0",
    "@types/node": "^14.14.6",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.28.4",
    "cz-conventional-changelog": "^3.3.0",
    "danger": "^10.6.6",
    "eslint": "^7.31.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^7.0.1",
    "jest": "^27.0.6",
    "lint-staged": "^11.1.0",
    "prettier": "^2.3.2",
    "semantic-release": "^17.4.4",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "@aws/dynamodb-data-mapper": "^0.7.3",
    "@aws/dynamodb-data-mapper-annotations": "^0.7.3",
    "@types/jest": "^26.0.24",
    "aws-sdk": "^2.953.0",
    "dotenv": "^10.0.0",
    "ts-jest": "^27.0.4"
  },
  "files": [
    "lib/**/*",
    "CHANGELOG.md",
    "package.json",
    "yarn.lock"
  ],
  "scripts": {
    "start": "npx tsc && node -r dotenv/config lib/index.js",
    "test": "jest",
    "build": "tsc",
    "lint": "eslint '*/**/*.{js,ts,tsx}' --quiet --fix",
    "cz": "cz",
    "semantic-release": "semantic-release"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
